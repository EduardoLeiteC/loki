    },
    "author": "Charles Viegas",
    "license": "ISC",
    "dependencies": {
        "accounting": "^0.4.1",
        "moment": "^2.22.2",
        "v-money": "^0.8.1",
        "vue": "^2.5.16",
        "vuetify": "^1.1.5",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-jest": "^23.0.0",
        "babel-loader": "^7.1.2",
        "babel-preset-env": "^1.6.0",
        "babel-preset-stage-3": "^6.24.1",
        "cross-env": "^5.0.5",
        "css-loader": "^0.28.7",
        "file-loader": "^1.1.4",
        "jest": "^23.0.0",
        "jest-serializer-vue": "^1.0.0",
        "vue-jest": "^2.6.0",
